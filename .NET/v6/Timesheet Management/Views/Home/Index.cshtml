@using Timesheet_Management.Models
@model UserModel
<br />
<br />
@using (Html.BeginForm("Index", "Home", FormMethod.Post))
{
    <b>Select Tea Type: </b>
    <br />
    @Html.DropDownListFor(x => x.SelectedTeaIds, Model.TeaList, new { @multiple = "multiple", style = "width:200px" })
    <br />
    <input type="submit" value="submit" />
}

<h4>You Selected</h4>
<b style="color:red">Tea Type: @ViewBag.Message</b>